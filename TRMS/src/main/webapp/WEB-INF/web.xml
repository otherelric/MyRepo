    <servlet-name>PageNotFound</servlet-name>
    <url-pattern>/PageNotFound</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogOutServlet</display-name>
    <servlet-name>LogOutServlet</servlet-name>
    <servlet-class>com.revature.servlet.LogOutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogOutServlet</servlet-name>
    <url-pattern>/LogOutServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AddRequestServlet</display-name>
    <servlet-name>AddRequestServlet</servlet-name>
    <servlet-class>com.revature.servlet.FrontServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddRequestServlet</servlet-name>
    <url-pattern>/AddRequestServlet</url-pattern>
  </servlet-mapping>
</web-app>